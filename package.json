    "lint:js": "eslint --fix --ext .jsx,.js src -c configs/.eslintrc.js",
    "lint": "npm run lint:js && npm run lint:sass",
    "dev": "NODE_ENV=development webpack-dev-server --config ./webpack/development.js",
    "dev:win": "set NODE_ENV=development && webpack-dev-server --config ./webpack/development.js",
    "prod": "NODE_ENV=production webpack --config ./webpack/production.js",
    "prod:win": "set NODE_ENV=production && webpack --config ./webpack/production.js",
    "build": "npm run lint && npm run prod",
    "destroy": "rm -rf dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
